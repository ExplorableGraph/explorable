{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "checkJs": true,
    "noEmit": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "target": "ESNext"
  },
  "include": [
    // "packages/*/src/**/*.js",
    // "packages/*/test/**/*.js",
    // "test/core/boilerplate.tests.js",
    // "src/common/Cache.js",
    // "src/web/mediaTypes.js",
    // "src/node/CommandModules.js",
    // "src/eg/dot.js",
    // "test/eg/evaluate.tests.js",
    // "src/eg/evaluate.js"
    "src/core/*.js",
    "src/common/*.js",
    "test/core/*.js"
  ]
}
